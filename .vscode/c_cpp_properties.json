{
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${default}",
                "${workspaceFolder}/**",
                "${workspaceFolder}",
                "${workspaceFolder}/include",
                "${workspaceFolder}/include/sys",
                "${workspaceFolder}/include/IO",
                "${workspaceFolder}/sysheaders",
                "${workspaceFolder}/src"
            ],
            "defines": [],
            "macFrameworkPath": [
                "/System/Library/Frameworks",
                "/Library/Frameworks"
            ],
            "compilerPath": "/usr/bin/clang",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "macos-clang-x64",
            "browse": {
                "path": [
                    "${workspaceFolder}/**",
                    "/System/Library/Frameworks",
                    "/Library/Frameworks"
                ],
                "limitSymbolsToIncludedHeaders": false
            }
        }
    ],
    "version": 4
}
